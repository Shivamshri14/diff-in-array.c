#include<stdio.h>
#include<conio.h>
int main()
{
    int A[5],B[5],i,j,f;
    printf("Enter elements of first array\n");
    for(i=0;i<5;i++ )
    scanf("%d",&A[i]);
    printf("Enter elements of second array\n");
    for(i=0;i<5;i++ )
    scanf("%d",&B[i]);
     //flag = 0;
    for (i=0;i<5;i++)               
    {        
         f = 0;
        for (j=0;j<5;j++)                                
        {
            if (A[i]==B[j])
            {
                f = 1;
                break;                                                   
            }
        }
        if(f==0)
        {
            printf("difference between 2 arrays are %d\n",A[i]);
        }
    }
    
}
